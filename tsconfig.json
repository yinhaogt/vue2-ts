{
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "./dist/",
    //"noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "preserve",
    "allowJs": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
